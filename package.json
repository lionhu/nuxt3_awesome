{
  "name": "@nuxt-awesome/theme",
  "version": "2.0.6",
  "description": "a Nuxt 3 template and boilerplate with a lot of useful features. Nuxt 3 + Tailwindcss + Nuxt Layer",
  "author": "viandwi24",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/viandwi24/nuxt3-awesome-starter"
  },
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "modules",
    "pages",
    "public",
    "stores",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tailwind.config.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "nuxt build ./app",
    "dev": "nuxt dev ./app --no-clear",
    "generate": "nuxt generate ./app",
    "preview": "nuxt preview ./app",
    "postinstall": "nuxt prepare ./app",
    "dev:starter": "nuxt dev ./examples/starter",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt/content": "^2.7.0",
    "@nuxt/devtools": "^0.6.7",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/strapi": "^1.6.3",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.2",
    "@pinia/nuxt": "^0.4.11",
    "@vueuse/components": "^9.13.0",
    "@vueuse/core": "^10.2.0",
    "@vueuse/motion": "2.0.0-beta.25",
    "@vueuse/nuxt": "^10.2.1",
    "@windicss/plugin-animations": "^1.0.9",
    "chart.js": "^3.1.0",
    "google-auth-library": "^8.9.0",
    "meilisearch": "^0.34.2",
    "nuxt-headlessui": "^1.1.4",
    "nuxt-icon": "^0.4.2",
    "nuxt-swiper": "^1.0.0",
    "nuxt-windicss": "^2.6.0",
    "pinia": "^2.1.4",
    "sass": "^1.63.4",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "stringify": "^5.2.0",
    "sweetalert2": "^11.6.10",
    "tailwindcss": "^3.3.2",
    "unplugin-icons-nuxt3": "^1.0.0",
    "unplugin-vue-components": "^0.25.1",
    "v-calendar": "^3.0.0-alpha.8",
    "vee-validate": "^4.7.3",
    "vue": "^3.2.41",
    "vue-chart-3": "^3.1.8",
    "vue3-carousel-nuxt": "^1.1.0"
  },
  "devDependencies": {
    "@googlemaps/js-api-loader": "^1.15.1",
    "@googlemaps/markerclusterer": "^2.0.15",
    "@iconify/json": "^2.1.135",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/i18n": "8.0.0-rc.4",
    "@types/google.maps": "^3.51.1",
    "@vee-validate/i18n": "^4.7.3",
    "@vee-validate/rules": "^4.7.3",
    "consola": "^3.1.0",
    "daisyui": "^3.7.5",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "^3.6.1",
    "prettier": "^2.8.8",
    "release-it": "^15.11.0",
    "serve": "^14.2.0",
    "typescript": "^5.1.3",
    "vite": "^3.1.0 || ^4.0.0",
    "vue-tsc": "1.8.1",
    "vue3-google-map": "^0.15.0",
    "webpack": ">=5.0.0 <6.0.0"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  }
}